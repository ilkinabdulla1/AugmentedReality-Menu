<main class="main-content" style="padding: 20px; background-color: #f5f5f5;">
  <div class="container" style="background: #fff; padding: 30px; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);">
    <h1 style="margin-bottom: 20px;">Menu Items Pending 3D Models</h1>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>Menu Item</th>
          <th>Category</th>
          <th>Restaurant</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% menuItems.forEach((item, index) => { %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= item.name %></td>
            <td><%= item.category ? item.category.name : 'N/A' %></td>
            <td><%= item.restaurant ? item.restaurant.name : 'N/A' %></td>
            <td>
              <!-- Upload 3D Model -->
              <form
                action="/menu-management/<%= item._id %>/upload-3d-inline"
                method="POST"
                enctype="multipart/form-data"
                style="display: flex; align-items: center;"
              >
                <input
                  type="file"
                  name="threeDFile"
                  accept=".obj,.glb,.gltf"
                  style="display: none;"
                  onchange="this.form.submit();"
                  id="upload-<%= item._id %>"
                />
                <label
                  for="upload-<%= item._id %>"
                  class="btn btn-primary"
                  style="padding: 5px 10px; border-radius: 5px; font-size: 0.9rem; cursor: pointer;"
                >
                  Upload
                </label>
              </form>
            </td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</main>
