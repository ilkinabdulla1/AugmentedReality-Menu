<main class="main-content" style="min-height: 100vh; background-color: #f5f7fa; padding: 20px;">
    <div class="container" style="max-width: 1200px; background: #ffffff; padding: 40px; padding-right: 300px; border-radius: 15px; box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);">
      <div class="page-header" style="text-align: center; margin-bottom: 40px;">
        <h1 style="font-size: 2.5rem; font-weight: bold; color: #333;">All Menu Items</h1>
        <p style="font-size: 1.2rem; color: #777;">Browse menu items categorized</p>
      </div>
  
      <% if (Object.keys(menuItemsByCategory).length > 0) { %>
        <% Object.entries(menuItemsByCategory).forEach(([categoryName, menuItems]) => { %>
          <div style="margin-bottom: 30px;">
            <h2 style="font-size: 1.5rem; color: #4CAF50; border-bottom: 2px solid #ddd; padding-bottom: 5px;"><%= categoryName %></h2>
            <ul style="list-style: none; padding: 0;">
              <% if (menuItems.length > 0) { %>
                <% menuItems.forEach(menuItem => { %>
                  <li style="padding: 10px; background: #f9f9f9; border-radius: 8px; margin-bottom: 10px;">
                    <strong><%= menuItem.name %></strong> - $<%= menuItem.price.toFixed(2) %> <br>
                    <small>Restaurant: <%= menuItem.restaurant.name %></small>
                  </li>
                <% }) %>
              <% } else { %>
                <li style="padding: 10px; color: #999;">No menu items in this category</li>
              <% } %>
            </ul>
          </div>
        <% }) %>
      <% } else { %>
        <p style="text-align: center; color: #999;">No menu items available</p>
      <% } %>
    </div>
  </main>
  